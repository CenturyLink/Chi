h3 Portal -- Accordion Expanded -- Child Expanded
.example.-mb--3
  .-p--3
    .chi-card.-portal.-bg--white.-bg-md--grey-15
      .chi-card__header.-sm
        .chi-card__title Title
      .chi-card__content.-p--0
        .chi-data-table.-portal.-compact
          .chi-data-table__head
            .chi-data-table__row
              .chi-data-table__cell.-expandable
              .chi-data-table__cell
                div Name
              .chi-data-table__cell
                div ID
              .chi-data-table__cell
                div Last Login
          .chi-data-table__body
            .chi-data-table__row.-collapsed.-xl
              .chi-data-table__cell.-expandable
                button.chi-button.-icon.-flat.-sm(aria-label='Expand row')
                  .chi-button__content
                    i.chi-icon.icon-plus
              .chi-data-table__cell.-key.-bold
                div Name 1
              .chi-data-table__cell.-key
                div name-1
              .chi-data-table__cell
                div Dec 18, 2020 3:26 PM
            .chi-data-table__row.-expanded.-striped.-xl
              .chi-data-table__cell.-expandable
                button.chi-button.-icon.-flat.-sm(aria-label='Expand row')
                  .chi-button__content
                    i.chi-icon.icon-minus
              .chi-data-table__cell.-key.-bold
                div Name 2
              .chi-data-table__cell.-key
                div name-2
              .chi-data-table__cell
                div Dec 18, 2020 2:38 AM
            .chi-data-table__row-child.-expanded.-xl
              .chi-data-table__cell.-expandable
              .chi-data-table__cell.-key.-bold
                button.chi-button.-icon.-flat.-sm.-expand(aria-label='Expand row')
                  .chi-button__content
                    i.chi-icon.icon-minus
                div Child Name 1
              .chi-data-table__cell.-key
                div Child name-1
              .chi-data-table__cell
                div Dec 18, 2020 2:38 AM
            .chi-data-table__row-grand-child.-xl
              .chi-data-table__cell.-expandable
              .chi-data-table__cell.-key.-bold
                div Grand-child Name 1
              .chi-data-table__cell.-key
                div Grand-child name-1
              .chi-data-table__cell
                div Dec 18, 2020 2:38 AM
            .chi-data-table__row-child.-collapsed.-xl
              .chi-data-table__cell.-expandable
              .chi-data-table__cell.-key.-bold
                div Child Name 2
              .chi-data-table__cell.-key
                div Child name-2
              .chi-data-table__cell
                div Dec 18, 2020 2:38 AM
            .chi-data-table__row.-collapsed.-xl
              .chi-data-table__cell.-expandable
                button.chi-button.-icon.-flat.-sm(aria-label='Expand row')
                  .chi-button__content
                    i.chi-icon.icon-plus
              .chi-data-table__cell.-key.-bold
                div Name 3
              .chi-data-table__cell.-key
                div name-3
              .chi-data-table__cell
                div Nov 5, 2020 10:15 AM
          .chi-data-table__footer
            nav.chi-pagination.-compact(role='navigation', aria-label='Pagination')
              .chi-pagination__content
                .chi-pagination__start
                  .chi-pagination__results
                    span.chi-pagination__label 240 Results
                .chi-pagination__center
                  .chi-pagination__button-group.chi-button-group
                    button.chi-button.-icon.-flat.-primary.-sm(aria-label='First page')
                      .chi-button__content
                        i.chi-icon.icon-page-first(aria-hidden='true')
                    button.chi-button.-icon.-flat.-primary.-sm(aria-label='Previous page')
                      .chi-button__content
                        i.chi-icon.icon-chevron-left(aria-hidden='true')
                  .chi-pagination__label
                    strong 2
                    span of
                    strong 3
                  .chi-pagination__button-group.chi-button-group
                    button.chi-button.-icon.-flat.-primary.-sm(aria-label='Next page')
                      .chi-button__content
                        i.chi-icon.icon-chevron-right(aria-hidden='true')
                    button.chi-button.-icon.-flat.-primary.-sm(aria-label='Last page')
                      .chi-button__content
                        i.chi-icon.icon-page-last(aria-hidden='true')
                .chi-pagination__end

