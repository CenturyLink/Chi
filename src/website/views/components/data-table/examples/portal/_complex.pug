script.
  const namePopover = Vue.component("namepopover", {
    props: ["name", "id"],
    popoverAnimationTimeout: null,
    mounted() {
      const buttonOpenOnHover = document.getElementById(
        `name-popover-button-${this.id}`
      );
      const popover = window.chi.popover(buttonOpenOnHover);
      let hoverAnimationTimeout;

      if (buttonOpenOnHover) {
        buttonOpenOnHover.addEventListener("mouseenter", function () {
          hoverAnimationTimeout = setTimeout(() => {
            popover.show();
          }, 300);
        });

        buttonOpenOnHover.addEventListener("mouseleave", function () {
          if (hoverAnimationTimeout) {
            clearTimeout(hoverAnimationTimeout);
          }
          popover.hide();
        });
      }
    },
    beforeDestroy() {
      if (this.popover) {
        this.popover.dispose();
