{
  "name": "ux-chi-custom-elements",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "module": "dist/index.mjs",
  "main": "dist/index.cjs.js",
  "unpkg": "dist/ionic.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --docs-json /docs/docs.json",
    "start.es5": "stencil build --dev --watch --serve --es5",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "prettier": "prettier --write src/**/*.tsx"
  },
  "dependencies": {
    "all-country-data": "1.0.3",
    "dayjs": "^1.11.12",
    "libphonenumber-js": "^1.11.4",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "@stencil/core": "^4.19.2",
    "@stencil/sass": "^3.0.12",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.11",
    "prettier": "^3.3.3",
    "tslint": "^6.1.3",
    "uuid": "^10.0.0"
  },
  "license": "MIT"
}
