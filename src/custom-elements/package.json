{
  "name": "ux-chi-custom-elements",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "module": "dist/index.mjs",
  "main": "dist/index.cjs.js",
  "unpkg": "dist/ionic.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --docs-json /docs/docs.json",
    "start.es5": "stencil build --dev --watch --serve --es5",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "prettier": "prettier --write src/**/*.tsx"
  },
  "dependencies": {
    "all-country-data": "1.0.3",
    "dayjs": "1.11.10",
    "libphonenumber-js": "1.10.52",
    "popper.js": "1.16.1"
  },
  "devDependencies": {
    "@stencil/core": "4.9.0",
    "@stencil/sass": "3.0.7",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "prettier": "^3.1.1",
    "tslint": "5.16.0"
  },
  "license": "MIT"
}
